    "bootstrap-sass": "^3.3.7",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "electron": "^1.4.5",
    "electron-packager": "^8.2.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "node-sass": "^3.11.0",
    "sass-loader": "^4.0.2",
    "script-loader": "^0.7.0",
